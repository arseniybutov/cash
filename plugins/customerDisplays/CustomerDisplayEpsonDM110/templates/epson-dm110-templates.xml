           xsi:schemaLocation="http://crystals.ru/pos/customerdisplay/templates/parser fec-templates.xsd">
    <screen name="addPositionStart">
        <line>
            <item type="element" row="1" width="10" align="left" element="productName"/>
            <item type="element" row="12" width="10" align="right" element="productPrice"/>
