<?xml version="1.0" encoding="UTF-8"?>
<moduleConfig xsi:schemaLocation="http://crystals.ru/cash/settings ../../module-config.xsd"
              settingsGroup="equipment" visible="true"
              description="Драйвер сканера через порты PS/2 и USB" xmlns="http://crystals.ru/cash/settings"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <property key="provider" value="ps2"/>
